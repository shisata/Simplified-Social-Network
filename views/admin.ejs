<%- include('menu_buttons') %>
<body>
    <div>
        <h1>All users</h1>
        <ul>
            <% users.forEach(function(user) { %>
                <li>
                    <%= user.email %>
                    <% if(user.active==true) { %>
                        online
                        <% } else { %>
                            offline
                            <% } %>
                </li>
                <% }); %>
        </ul>
    </div>
    
</body>